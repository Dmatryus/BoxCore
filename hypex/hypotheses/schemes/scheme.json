          ],
          "properties": {
            "executors": {
              "type": "array",
              "title": "The executors Schema",
              "items": {
                "type": "object"
              }
            },
            "role": {
              "enum": [
                "Info",
                "Target",
                "Treatment",
                "Grouping",
                "Stratification",
                "PreTarget"
              ],
              "default": "Target",
              "title": "The role Schema"
            }
          }
        },
        "OneAASplitAnalyzer": {
          "properties": {}
        },
        "Shuffle": {
