            "grouping_role": {
              "default": "Treatment",
              "enum": [
                "Info",
                "Target",
                "Treatment",
                "Grouping",
                "Stratification",
                "PreTarget"
              ]
            },
            "space": {
              "default": "auto",
              "enum": [
                "data",
                "additional",
                "auto"
              ]
            }
          }
        },
        "KSTest": {
          "required": [],
