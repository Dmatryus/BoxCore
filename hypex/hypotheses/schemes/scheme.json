                "Info",
                "Target",
                "Treatment",
                "Grouping",
                "Stratification",
                "PreTarget"
              ]
            },
            "space": {
              "default": "auto",
              "enum": [
                "data",
                "additional",
                "auto"
              ]
            },
            "reliability": {
              "type": "number",
              "default": 0.05
            }
          }
        },
        "Max": {
          "properties": {}
        },
        "Mean": {
          "properties": {}
        },
        "Median": {
          "properties": {}
        },
        "MetricAbsoluteDelta": {
          "properties": {}
        },
        "MetricDelta": {
          "properties": {}
        },
        "MetricLogRatio": {
          "properties": {}
        },
        "MetricPercentageDelta": {
          "properties": {}
        },
        "MetricPercentageRatio": {
          "properties": {}
        },
        "MetricRatio": {
          "properties": {}
        },
        "MetricRelativeDelta": {
          "properties": {}
        },
        "Min": {
          "properties": {}
        },
        "Mode": {
          "properties": {}
        },
        "OnRoleExperiment": {
          "required": [
            "executors",
