                "Target",
                "Treatment",
                "Grouping",
                "Stratification",
                "PreTarget"
              ]
            },
            "space": {
              "default": "auto",
              "enum": [
                "data",
                "additional",
                "auto"
              ]
            },
            "reliability": {
